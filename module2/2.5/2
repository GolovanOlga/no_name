#include <stdio.h>
void F (char * x, char * y);
int main ()
{
	char str1[10], str2[10];
	int i=0, j=0;
    scanf ("%s %s", str1, str2);
    void F (char * x, char * y) {
    	*y=*x;
    }
    while(str1[i]!='\0'){
        j++;
        i++;
    }
    i=0;
    j=j-1;
    while (str1[i]!='\0'){
    F (&str1[i], &str2[j]);
    i++;
    j--;}
    printf("%s %s\n",str1, str2);
	return 0;
}
